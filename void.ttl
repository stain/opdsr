:oidd-dataset
        a                           void:Dataset , dctypes:Dataset ;
        dcterms:accrualPeriodicity  "Irregular" ;
        dcterms:description         "The OIDD Semantic Knowledge Network encodes the experimental relationship between NPC compounds and OIDD assays in a standardized format that can be exported into other databases and information sources. Similarly, it also encodes relationships between compounds and assays and among other experimental data, including ChEMBL dataset mapping and classification of assays using the bioassay ontology (BAO)."@en ;
        dcterms:issued              "2015-10-21T00:00:00.798Z"^^xsd:dateTime ;
        dcterms:license             <http://creativecommons.org/licenses/by-sa/3.0/> ;
        dcterms:publisher           <https://ncats.nih.gov/> ;
        ## FIXME: What is a good and global enough title?
        dcterms:title               "OIDD Semantic Knowledge Network"@en ;
        # FIXME: Who contributed how?
        pav:createdBy               <http://orcid.org/0000-0001-7403-8819>, <http://orcid.org/0000-0002-1476-6192> ;
        pav:curatedBy               <http://orcid.org/0000-0002-1476-6192> ;
        pav:derivedFrom             <http://www.bioassayontology.org/bao/bao_complete.owl> ,
          <http://rdf.ncbi.nlm.nih.gov/pubchem/void.ttl#bioassay> ,
          <http://rdf.ncbi.nlm.nih.gov/pubchem/void.ttl#endpoint> ,
          <http://rdf.ncbi.nlm.nih.gov/pubchem/void.ttl#substance> ,
          <http://rdf.ebi.ac.uk/dataset/chembl/20.0/void.ttl#chembl_rdf_target_dataset>
          <http://rdf.ebi.ac.uk/dataset/chembl/20.0/void.ttl#chembl_rdf_activity_dataset> ;
        pav:previousVersion         <https://spotlite.nih.gov/ncats/opdsr/commits/20150209> ;
        ## FIXME: Need version number or date?
        pav:version                 "0.0.1" ;
        ## FIXME: Should be to a tagged/versioned download
        void:dataDump               <https://spotlite.nih.gov/ncats/opdsr/repository/archive.zip?ref=master> ;
        dcat:landingPage            <https://ncats.nih.gov/expertise/preclinical/pd2#semantic> ;
        dcat:distribution
            :NPC-OIDD-Assay-csv,
            :NPC-OIDD-Compound-csv,
            :NPC-OIDD-Results-csv .


